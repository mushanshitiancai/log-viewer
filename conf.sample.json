{
	"logs":{
		"test":{
			"paths"            : [ "test" ],
			"file_regex"       : [ "input" ],
			"log_header_regex" : "^DEBUG|^TRACE|^NOTICE|^WARNING|^FATAL",
			"log_time_regex"   : "\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}"
		}
	},
	"search_color": "bgRed",
	"tag_line_color": "order",
	"tail_separator": "\\n(?=DEBUG)|\\n(?=TRACE)|\\n(?=NOTICE)|\\n(?=WARNING)|\\n(?=FATAL)"
}